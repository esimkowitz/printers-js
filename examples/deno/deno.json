{
  "tasks": {
    "start": "deno run --allow-env --allow-read --allow-net --allow-ffi main.ts",
    "dev": "deno run --allow-env --allow-read --allow-net --allow-ffi --watch main.ts"
  },
  "imports": {
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.7",
    "@printers/printers": "npm:@printers/printers@^0.8",
    "@std/path": "jsr:@std/path@^1.1.2"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "strict": true
  }
}
