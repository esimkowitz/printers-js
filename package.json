{
  "name": "@printers/printers",
  "version": "0.5.1",
  "description": "Cross-platform printer library for Node.js, Deno, and Bun",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "napi": {
    "binaryName": "printers",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu"
    ]
  },
  "engines": {
    "node": ">= 20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esimkowitz/printers-js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@cross/test": "npm:@jsr/cross__test@^0.0.10",
    "@napi-rs/cli": "^3.1.5",
    "@std/assert": "npm:@jsr/std__assert@^1.0.14",
    "@types/bun": "^1.2.21",
    "@types/node": "^24.3.1",
    "@types/prettier": "^2.7.3",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "c8": "^10.1.2",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "packageManager": "npm@10.0.0",
  "scripts": {
    "compile": "node scripts/compile.js",
    "build": "node scripts/build-all.js",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "prepublishOnly": "npm run compile && task prepublish"
  }
}
